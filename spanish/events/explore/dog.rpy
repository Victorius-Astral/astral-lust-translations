    # game/events/explore/dog.rpy:12
    old "Let's check"
    new "Revisar"

    # game/events/explore/dog.rpy:31
    old "Investigate"
    new "Investigar"

    # game/events/explore/dog.rpy:55
    old "Save the dog"
    new "Salva al perro"

